pipeline{
    agent any

    stages{

        stage('Get code'){
            steps{
                git branch:'main', 
                    url: 'https://github.com/dhingradeepanshu/demoDevopsRepo'
            }
        }

        stage('Build Code'){
            steps{
                sh '''
                bat ./mvnw package
                '''
            }
        }

        


    }
}